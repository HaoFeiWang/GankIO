<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.whf.gankio.View.Activitys.StudyActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/study_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/light_blue"
        app:theme="@style/StudyActivityTheme">
    </android.support.v7.widget.Toolbar>
    <FrameLayout
        android:id="@+id/progress_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible">
        <com.whf.gankio.View.me.fichardu.circleprogress.CircleProgress
            android:id="@+id/progress"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:color1="@android:color/holo_blue_dark"
            app:color2="@android:color/holo_red_dark"
            app:color3="@android:color/holo_orange_dark" />
    </FrameLayout>

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </WebView>
</LinearLayout>
